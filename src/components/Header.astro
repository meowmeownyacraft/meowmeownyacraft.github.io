---
import Nav from "./Nav.astro";

const { class: className } = Astro.props;
---

<header class:list={[className]}>
    <a href="/" class="grow items-stretch flex">
        <img
            src="/logo.svg"
            alt=""
            class="pt-4 mx-auto w-48 h-32 object-contain self-center shadow-2xl"
            loading="eager"
        />
    </a>

    <Nav />
</header>
